{
  "name": "meetnow",
  "description": "An open source, peer-to-peer video calling service alternative to Zoom.",
  "author": "MeetNow Team <meetnow@yeho.dev> (https://meetnow.yeho.dev)",
  "contributors": [
    "Yehonatan Rosenblatt <meetnow@yeho.dev> (https://yeho.dev)",
    "Alon Green <alongreenalgr@gmail.com>"
  ],
  "version": "0.0.1",
  "main": "public/build/main.js",
  "private": true,
  "scripts": {
    "build": "tsc -p src/electron & rollup -c",
    "dev": "tsc -w -p src/electron & rollup -c -w",
    "build-svelte": "rollup -c",
    "build-electron": "tsc -p src/electron",
    "dev-svelte": "rollup -c -w",
    "dev-electron": "tsc -w -p src/electron",
    "start": "electron public/build/main.js",
    "dist": "yarn build && electron-builder",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "@types/ffi-napi": "^4.0.5",
    "@types/w3c-image-capture": "^1.0.5",
    "electron": "17.0.1",
    "electron-builder": "^22.14.5",
    "ffi-napi": "^4.0.3",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^2.0.2",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  }
}
